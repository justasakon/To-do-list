{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
 "parser": "@babel/eslint-parser",
"parserOptions": {
  "requireConfigFile": false,
  "ecmaVersion": "latest",
  "sourceType": "module"
},
 "extends": ["airbnb-base"],
  "rules": {
    "no-shadow": "off",
    "no-param-reassign": "off",
    "eol-last": "off",
    "import/extensions": [ 1, {
      "js": "always", "json": "always"
    }]
  },
  "ignorePatterns": [
    "dist/",
    "build/",
    "public/bundle.js"
  ]
}